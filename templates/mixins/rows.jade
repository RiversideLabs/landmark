include columns

mixin row(list, colums, item)
  tr(id=item.id)
    if !list.get('nodelete')
      td.control: a(href='/landmark/' + list.path + '?delete=' + item.id + csrf_query).control-delete
    if sortable
      td.control: a(href=js).control-sort
    each col, i in columns
      td
        +column(list, col, item)
